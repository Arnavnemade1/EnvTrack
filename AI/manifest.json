{
  "manifest_version": 3,
  "name": "EnviroTrack AI Monitor",
  "version": "2.0.0",
  "description": "Track environmental impact of your AI usage with beautiful insights",
  "permissions": [
    "webRequest",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
